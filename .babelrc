{
    "presets": [
        ["@babel/preset-typescript", { "onlyRemoveTypeImports": true }],
        "@babel/preset-env",
        "minify"
    ],
    "plugins": [
        "./dist/babel",
        "@babel/plugin-transform-modules-commonjs",
        "@babel/plugin-syntax-jsx",
        ["@babel/plugin-transform-react-jsx", { "pragma": "jsx.render" }]
    ]
}
